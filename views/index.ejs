<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Servicios profesionales de limpieza, garantizando calidad y satisfacción en cada rincón. Contáctanos hoy.">
    <meta name="keywords" content="limpieza, servicios de limpieza, limpieza profesional, limpieza comercial">
    <meta name="author" content="Professional Cleaning Services">
    <meta name="robots" content="index, follow">
    <title><%= title %></title>
    <link rel="stylesheet" href="/css/styles.css">
</head>
<body>
    
    <header class="hero">
        <div class="overlay"></div>
        <h1 class="hero-title"><%= header.mainHeading %></h1>
        <p><%= header.subHeading %></p>
        <!--<div class="hero-buttons">
            <a href="#services" class="btn">Our Services</a>
            <a href="#contact" class="btn">Contact Us</a>
        </div>-->
    </header>    

<!-- Navigation -->
<nav class="main-nav">
    <ul>
        <li><a href="#about">About</a></li>
        <li><a href="#services">Services</a></li>
        <li><a href="#testimonials">Testimonials</a></li>
        <li><a href="#contact">Contact</a></li>
    </ul>
</nav>



    <!-- About Section -->
    <section class="about" id="about">
        <h2><%= about.heading %></h2>
        <p><%= about.description %></p>
    </section>

    <!-- Services Section -->
    <section class="services" id="services">
        <h2><%= services.heading %></h2>
        <div class="services-grid">
            <% services.list.forEach(service => { %>
                <div class="service-card">
                    <img src="<%= service.imageUrl %>" alt="<%= service.name %>" style="width: 100%; border-radius: 5px;">
                    <h3><%= service.name %></h3>
                    <p><%= service.description %></p>
                </div>
            <% }); %>
        </div>
    </section>

<!-- Testimonials Section -->
